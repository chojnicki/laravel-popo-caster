{
    "name": "morrislaptop/laravel-castable-object",
    "description": "Automatically cast JSON columns to rich PHP objects in Laravel using Symfony's Serializer",
    "keywords": [
        "morrislaptop",
        "laravel-castable-object"
    ],
    "homepage": "https://github.com/morrislaptop/laravel-castable-object",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Morris",
            "email": "craig.michael.morris@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "spatie/laravel-event-sourcing": "^4.7",
        "spatie/laravel-ray": "^1.9",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Morrislaptop\\Caster\\": "src",
            "Morrislaptop\\Caster\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morrislaptop\\Caster\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Morrislaptop\\Caster\\CasterServiceProvider"
            ],
            "aliases": {
                "Caster": "Morrislaptop\\Caster\\CasterFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
